{
    "plugins": [],
    "recurseDepth": 10,
    "source": {
        "include": ["README.md", "server.js", "models", "controllers"],
        "exclude": ["./node_modules"],
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "sourceType": "module",
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc","closure"]
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    },
    "opts": {
      "tutorials": "tutorials"
    }
}
